@model   IEnumerable<OisisCarRental.Models.Booking>

<h2>Booking</h2>


       <table class="table table-bordered">



    <thead>
        <tr>
            <th> @Html.DisplayNameFor(model => model.Name) </th>
            <th> @Html.DisplayNameFor(model => model.FromDate) </th>
            <th> @Html.DisplayNameFor(model => model.ToDate) </th>
            <th> @Html.DisplayNameFor(model => model.CreatedOn) </th>
            <th> @Html.DisplayNameFor(model => model.S_address) </th>
            <th> @Html.DisplayNameFor(model => model.D_address) </th>
            <th> @Html.DisplayNameFor(model => model.Email_Bk) </th>
            <th> @Html.DisplayNameFor(model => model.Contact_No) </th>
            <th> @Html.DisplayNameFor(model => model.Amount) </th>
            <th>Payment</th>

        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
               
                <td>@item.Name</td>
                <td>@item.FromDate</td>
                <td>@item.ToDate</td>
                <td>@item.CreatedOn</td>
                <td>@item.S_address</td>
                <td>@item.D_address</td>
                <td>@item.Email_Bk</td>
                <td>@item.Contact_No</td>
                <td>@item.Amount</td>
                <td>@Html.ActionLink(" ", "Create","Payments",null,new { @class = "fa fa-credit-card " }) </td>
            </tr>

        }
    </tbody>
</table>


   
